{
  "name": "f-vue3-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "mock": "vite --mode mock",
    "build": "run-p type-check build-only",
    "test-build": "run-p type-check build-only-test",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build --mode production",
    "build-only-test": "vite build --mode test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "pinia": "^2.0.32",
    "vant": "^4.1.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.4",
    "rollup-plugin-external-globals": "^0.7.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "terser": "^5.16.9",
    "typescript": "~4.8.4",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-require-transform": "^1.0.12",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": "16.x"
  }
}
